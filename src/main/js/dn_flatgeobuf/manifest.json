{
    "name": "dn_flatgeobuf",
    "version": "4.12.3",
    "title": "${bundleName}",
    "description": "${bundleDescription}",
    "vendor": "con terra GmbH",
    "productName": "devnet-mapapps-faltgeobuf",
    "layer": "module",
    "i18n": [
        "bundle"
    ],
    "dependencies": {
        "generic":  "^3.21.3",
        "geojson":  "^3.21.3",
        "ol": "^3.21.3"
    },
    "components": [
        {
            "name": "HelloTool",
            "impl": "ct/tools/Tool",
            "provides": [
                "ct.tools.Tool"
            ],
            "propertiesConstructor": true,
            "properties": {
                "id": "helloTool",
                "title": "Click Me",
                "toolRole": "toolset",
                "clickHandler": "print"
            },
            "references": [
                {
                    "name": "handlerScope",
                    "providing": "sample_helloworld.Hello"
                }
            ]
        },
        {
            "name": "Hello",
            "provides": [
                "sample_helloworld.Hello"
            ],
            "properties": {
                "message": "${hello}"
            }
        }
    ]
}
